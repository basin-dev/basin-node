{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"paths": {
			"@sdk/*": ["src/basin-sdk/*"],
			"@lib/*": ["src/lib/*"],
			"@util/*": ["src/util/*"]
		}
	}
}
